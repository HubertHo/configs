call plug#begin('~/.config/nvim/plugins')
Plug 'tpope/vim-commentary'
Plug 'tpope/vim-fugitive'
call plug#end()

" Editor
colorscheme deus

set background=dark
set colorcolumn=100
set encoding=utf8
set guicursor=
set laststatus=0 ruler
set mouse=a
set number
set tabstop=8 softtabstop=0 expandtab shiftwidth=4 smarttab
set visualbell
